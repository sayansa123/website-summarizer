{
  "manifest_version": 3,
  "name": "Website Summerizer",
  "description": "Summerize any site automatically",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html"
  },
  "host_permissions": [
    "http://localhost:8000/*",
    "https://*/*",
    "http://*/*"
  ],
  "permissions": [
    "tabs"
  ]
}
